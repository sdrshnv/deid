{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "De-Id",
  "version": "0.1.0",
  "identifier": "com.svankudre.deid",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../build"
  },
  "app": {
    "windows": [
      {
        "title": "De-Id",
        "width": 1000,
        "height": 650
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "shortDescription": "Local text de-identification tool",
    "category": "Utility",
    "windows": {
      "nsis": {
        "installMode": "currentUser"
      }
    }
  }
}
